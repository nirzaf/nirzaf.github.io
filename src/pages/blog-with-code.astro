---
import BaseLayout from '@/layouts/BaseLayout'
import TitlePage from '@/components/TitlePage'

const title = 'Blog with Code'
const description = 'Blog posts with code examples and technical content'
---

<BaseLayout title={title} description={description}>
    <TitlePage title={title} />
    <div class="container mx-auto px-4" data-pagefind-body>
        <div class="prose dark:prose-invert max-w-none">
            <h2>Code Examples and Technical Content</h2>
            <p>Welcome to our technical blog section where we share code examples and in-depth technical content.</p>
            <p>This section contains various technical articles and code examples to help you learn and implement different programming concepts.</p>
        </div>
    </div>
</BaseLayout>